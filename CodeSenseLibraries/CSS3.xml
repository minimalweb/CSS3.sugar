<?xml version="1.0" encoding="UTF-8"?>
<codesense version="1.0">
	<behavior>
		<append-dynamic>
			<matched-suffix>\s*(;)?</matched-suffix>
			<transform-into>$0(?1::;)</transform-into>
		</append-dynamic>
		<confirm characters=";" />
		<partial-confirm characters="-" />
	</behavior>
	
	<set name="de.minimalweb.css3.property.clipping-values">
	  <completion string="border-box" />
	  <completion string="padding-box" />
	  <completion string="content-box" />
	</set>
	
	<set name="de.minimalweb.css3.property.background">
		<completion string="linear-gradient">
		  <behavior>
				<append-dynamic>
					<matched-suffix>(\s*:)|[^:]</matched-suffix>
					<transform-into>(?1::(${1:bg-position angle}, ${2:color-stop}, ${3:color-stop})$4)</transform-into>
				</append-dynamic>
				<partial-confirm characters="-" />
		  </behavior>
		</completion>
		<completion string="radial-gradient" />
		<completion string="-moz-linear-gradient">
		  <behavior>
				<append-dynamic>
					<matched-suffix>(\s*:)|[^:]</matched-suffix>
					<transform-into>(?1::(${1:bg-position angle}, ${2:color-stop}, ${3:color-stop})$4)</transform-into>
				</append-dynamic>
				<partial-confirm characters="-" />
		  </behavior>
		</completion>
		<completion string="-moz-radial-gradient" />
		<completion string="-webkit-gradient">
		  <behavior>
				<append-dynamic>
					<matched-suffix>(\s*:)|[^:]</matched-suffix>
					<transform-into>(?1::(${1:type}, ${2:bg-position angle}, from(${3:color}), to(${4:color})${5:color-stop(${6:position}, ${7:color})})$8)</transform-into>
				</append-dynamic>
				<partial-confirm characters="-" />
		  </behavior>
		</completion>
	</set>
	
	<set name="de.minimalweb.css3.property.background-attachment">
	  <completion string="local" />
	</set>
	
