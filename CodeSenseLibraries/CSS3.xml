<?xml version="1.0" encoding="UTF-8"?>
<codesense version="1.0">
	<behavior>
		<append-dynamic>
			<matched-suffix>\s*(;)?</matched-suffix>
			<transform-into>$0(?1::;)</transform-into>
		</append-dynamic>
		<confirm characters=";" />
		<partial-confirm characters="-" />
	</behavior>
	
